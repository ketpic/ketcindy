        \fi
      \end{zahyou*}%
    }%
  \fi
  \ifnum\DHS@dp>\z@
    \def\DHS@i{-1}%
    \expandafter\@for\expandafter\DHS@c\expandafter:\expandafter=#3\do{%
      \Incr\DHS@i
      \ifnum\DHS@i=\z@\else\DHS@kugirisi\fi
