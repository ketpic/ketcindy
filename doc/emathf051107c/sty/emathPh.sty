    \edef\@x{#4}\edef\@y{#5}\def\@resen{}%
    \For\@t{\@hazimekaku}{\@owarikaku}{#3}\Do{%\Add\@t{\@lx}\@@t%
      \Cos\@t\@xi\Mul\@x\@xi\@xi
      \Sin\@t\@yi\Mul\@y\@yi\@yi
      \edef\@resen{\@resen(\@xi,\@yi)}}%
    \Cos\@owarikaku\@xi\Mul\@x\@xi\@xi\Sin\@owarikaku\@yi\Mul\@y\@yi\@yi
    \edef\@resen{\@resen(\@xi,\@yi)}%
